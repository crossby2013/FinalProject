@model IEnumerable<FinalProject.Models.Project>

@{
    ViewBag.Title = "Index";
}


    <h2><font size="8">My Projects</font></h2>

    <p>

    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Image)
            </th>
            <th>
                <b>
                    @Html.DisplayNameFor(model => model.Name)
                </b>
            </th>
            <th>
                <b>
                    @Html.DisplayNameFor(model => model.Description)
                </b>
            </th>

            <th>
                <b>
                    @Html.DisplayNameFor(model => model.Skills)
                </b>
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GitHub)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <img src="@Html.DisplayFor(modelItem => item.Image)">

                    
                </td>
               
                    <td>
                    <b>
                        @Html.DisplayFor(modelItem => item.Name)
                    </b>
                </td>
                <td>
                    <b>
                        @Html.DisplayFor(modelItem => item.Description)
                    </b>
                </td>

                <td>
                    <b>
                        @Html.DisplayFor(modelItem => item.Skills)
                    </b>
                </td>
                <td>
                    <a href="@Html.DisplayFor(modelItem => item.GitHub)"><img src="data:image/jpeg;base64," style="width:50px;height:50px;border:0;"></a>
                </td>
               
                    

                <td><b>
   
    @Html.ActionLink("Details", "Details", new { id = item.ProjectsID }) |
    
</b>
                </td>
                
            </tr>
        }
       
</table>
<div>
    <h1>Click on the Screenshot</h1>
    <a href="http://localhost:51378/index.html">
        <img src="/Content/Images/Musiq.PNG" />
    </a>
 
</div>


